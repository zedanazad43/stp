{
  "version": 2,
  "builds": [
    {
      "src": "server/_core/index.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": ["server/**", "drizzle/**"]
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/_core/index.ts"
    }
  ]
}
